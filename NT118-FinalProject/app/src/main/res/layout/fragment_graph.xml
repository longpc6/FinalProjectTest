<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/it_Graph"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="@drawable/rectangle_gradient_s_bluegray_800_e_gray_900"
    android:elevation="150dp"
    tools:context=".dashboardelements.GraphFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameAttribute"
        android:layout_width="340dp"
        android:layout_height="51dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="32dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/rectangle_bg_blue_50_radius_26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/img_btn_Attribute"
            android:layout_width="@dimen/_22pxh"
            android:layout_height="@dimen/_10pxh"
            android:layout_gravity="bottom|end"
            android:layout_marginHorizontal="@dimen/_19pxh"
            android:layout_marginVertical="@dimen/_19pxv"
            android:layout_marginStart="300dp"
            android:layout_marginTop="24dp"
            android:background="@drawable/img_down"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameDaystart"
        android:layout_width="340dp"
        android:layout_height="51dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rectangle_bg_blue_50_radius_26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameAttribute"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/img_btn_Daystart"
            android:layout_width="@dimen/_20pxh"
            android:layout_height="@dimen/_20pxh"
            android:layout_gravity="bottom|end"
            android:layout_marginHorizontal="19dp"
            android:layout_marginVertical="19dp"
            android:layout_marginStart="304dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/img_vector"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameDayEnd"
        android:layout_width="340dp"
        android:layout_height="51dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rectangle_bg_blue_50_radius_26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameDaystart"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/img_btn_DayEnd"
            android:layout_width="@dimen/_20pxh"
            android:layout_height="@dimen/_20pxh"
            android:layout_gravity="bottom|end"
            android:layout_marginHorizontal="19dp"
            android:layout_marginVertical="19dp"
            android:layout_marginStart="304dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/img_vector"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <Button
        android:id="@+id/btnShow"
        android:layout_width="107dp"
        android:layout_height="48dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="156dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/rectangle_gradient_s_deep_purple_a200_e_deep_purple_a700_radius_20_5"
        android:ellipsize="end"
        android:gravity="center"
        android:paddingStart="@dimen/_24pxh"
        android:paddingTop="@dimen/_8pxv"
        android:paddingEnd="@dimen/_24pxh"
        android:paddingBottom="@dimen/_8pxv"
        android:singleLine="true"
        android:fontFamily="@font/nunito_sans"
        android:text="Show"
        android:textAllCaps="false"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameDayEnd" />

